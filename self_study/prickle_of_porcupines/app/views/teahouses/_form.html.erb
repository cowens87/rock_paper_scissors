<%= form_with model: teahouse, url: teahouses_path, local: true do |f| %>
  <%= f.label :name, "Name:" %>
  <%= f.text_field :name%><br>
  <%= f.label :address, 'Address:' %>
  <%= f.text_field :address%><br>
  <%= f.label :city, 'City:' %>
  <%= f.text_field :city%><br>
  <%= f.label :state, 'State:' %>
  <%= f.text_field :state%><br>
  <%= f.label :zip, 'Zip:' %>
  <%= f.text_field :zip%><br>
  <%= f.submit button_text%>
<% end %>